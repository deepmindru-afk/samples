{
  "name": "01-agentcore-deployment",
  "version": "1.0.0",
  "description": "Deploying Strands Agents to Amazon Bedrock AgentCore Runtime",
  "type": "module",
  "main": "dist/agent.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/agent.js",
    "dev": "tsx src/agent.ts",
    "deploy": "tsx src/deploy-agent.ts",
    "invoke": "tsx src/invoke-agent.ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-agentcore": "^3.0.0",
    "@aws-sdk/client-bedrock-agentcore-control": "^3.0.0",
    "@strands-agents/sdk": "^0.1.2",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}